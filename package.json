{
  "name": "playscrape",
  "version": "0.0.3",
  "description": "",
  "type": "module",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "./dist/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "echo \"No tests\"",
    "dev": "node --no-warnings --enable-source-maps --loader ts-node/esm src/playscrape.ts",
    "build-types": "tsc ./src/index.ts --declaration --emitDeclarationOnly --skipLibCheck --esModuleInterop --outDir dist/",
    "build": "tsc && yarn build-types && chmod +x dist/playscrape.js",
    "release": "yarn build && changeset publish",
    "generate": "drizzle-kit generate:sqlite"
  },
  "bin": {
    "playscrape": "dist/playscrape.js"
  },
  "author": "John Resig <jeresig@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@commander-js/extra-typings": "^11.0.0",
    "@xmldom/xmldom": "^0.8.10",
    "better-sqlite3": "^8.4.0",
    "commander": "^11.0.0",
    "drizzle-orm": "^0.26.5",
    "fast-glob": "^3.3.2",
    "ora": "^6.3.1",
    "parse5": "^7.1.2",
    "playwright": "^1.32.3",
    "sharp": "^0.32.1",
    "xmlserializer": "^0.6.1",
    "xpath": "^0.0.33"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@sindresorhus/tsconfig": "^5.0.0",
    "@types/better-sqlite3": "^7.6.4",
    "@types/node": "^20.2.5",
    "@types/xmlserializer": "^0.6.5",
    "drizzle-kit": "^0.18.1",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
